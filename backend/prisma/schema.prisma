generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Entries {
  id                Int              @id @default(autoincrement())
  familyName        String
  personalName      String
  familyNameKana    String
  personalNameKana  String
  gender            String
  birthDay          DateTime
  prefecture        String
  tel               String
  email             String
  isAccompanied     Boolean
  visitDate         DateTime
  checkinTime       DateTime?
}